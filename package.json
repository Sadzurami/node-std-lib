{
  "name": "@sadzurami/std-lib",
  "version": "1.8.1",
  "description": "Standard library for my projects",
  "author": "Sadzurami (https://github.com/Sadzurami)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Sadzurami/node-std-lib"
  },
  "engines": {
    "node": ">=18"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    "./concurrency": {
      "require": "./dist/concurrency/index.js",
      "import": "./dist/concurrency/index.js",
      "types": "./dist/concurrency/index.d.ts"
    },
    "./resources": {
      "require": "./dist/resources/index.js",
      "import": "./dist/resources/index.js",
      "types": "./dist/resources/index.d.ts"
    },
    "./process": {
      "require": "./dist/process/index.js",
      "import": "./dist/process/index.js",
      "types": "./dist/process/index.d.ts"
    },
    "./steam": {
      "require": "./dist/steam/index.js",
      "import": "./dist/steam/index.js",
      "types": "./dist/steam/index.d.ts"
    }
  },
  "scripts": {
    "build": "del-cli dist && tsc",
    "prepare": "npm run build"
  },
  "dependencies": {
    "fs-extra": "^11.2.0",
    "p-queue": "^6",
    "read-pkg-up": "^7"
  },
  "devDependencies": {
    "@sadzurami/eslint-config": "^1.0.1",
    "@sadzurami/prettier-config": "^1.0.0",
    "@sadzurami/ts-config": "^1.0.1",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^18",
    "del-cli": "^6.0.0",
    "eslint": "^8",
    "prettier": "^3.4.2",
    "typescript": "^5.5.4"
  },
  "keywords": [
    "std-lib",
    "standard-library"
  ],
  "sideEffects": false
}
